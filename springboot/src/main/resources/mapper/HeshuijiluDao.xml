<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.HeshuijiluDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.HeshuijiluEntity" id="heshuijiluMap">
        <result property="biaoti" column="biaoti"/>
        <result property="heshuijilu" column="heshuijilu"/>
        <result property="jilushijian" column="jilushijian"/>
        <result property="yonghuzhanghao" column="yonghuzhanghao"/>
        <result property="yonghuxingming" column="yonghuxingming"/>
        <result property="touxiang" column="touxiang"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.HeshuijiluVO" >
		SELECT * FROM heshuijilu  heshuijilu         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.HeshuijiluVO" >
		SELECT  heshuijilu.* FROM heshuijilu  heshuijilu 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.HeshuijiluView" >

		SELECT  heshuijilu.* FROM heshuijilu  heshuijilu 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.HeshuijiluView" >
		SELECT * FROM heshuijilu  heshuijilu <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	


</mapper>
